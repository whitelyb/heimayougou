/**app.wxss**/

@import "./styles/iconfont.wxss";
/* 在微信小程序中 不支持 通配符 ‘ * ’ */

page, view, text, swiper, swiper-item, image, navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

page {
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  
  /* 定义统一字体大小 */
  font-size: 28rpx;
}

image {
  width: 100%;
}